{
	"name": "ReactDevStack",
	"version": "0.0.1",
	"description": "Base react stack for development",
	"private": true,
	"scripts": {
		"release-css": "node-sass --include-path scss style/style.scss | cleancss -o dist/bundle.css",
		"release": "npm run release-js && npm run release-css",
		"test": "karma start --single-run",
		"debug-test": "karma start",
		"build:webpack": "webpack --config webpack.config.prod.js --progress --colors --bail",
		"release-js": "npm run build:webpack",
		"start": "node devServer.js"
	},
	"author": "David Kadlec <amertak>(kadlec.david@gmail.com)",
	"license": "MIT",
	"homepage": "https://github.com/amertak/ReactDevStack",
	"dependencies": {
		"react": "^15.1.0",
		"react-dom": "^15.1.0"
	},
	"devDependencies": {
		"normalize-css": "^2.3.1",
		"postcss-loader": "^0.8.2",
		"autoprefixer": "^6.3.4",
		"babel-core": "^6.7.7",
		"babel-loader": "^6.2.1",
		"babel-plugin-proto-to-assign": "^1.0.4",
		"babel-plugin-syntax-flow": "^6.3.13",
		"babel-plugin-transform-flow-strip-types": "^6.4.0",
		"babel-plugin-transform-proto-to-assign": "^6.5.0",
		"babel-plugin-transform-object-rest-spread": "^6.3.13",
		"babel-plugin-typecheck": "^3.6.1",
		"babel-polyfill": "^6.3.14",
		"babel-preset-es2015": "^6.3.13",
		"babel-preset-react": "^6.3.13",
		"babel-preset-react-hmre": "^1.1.0",
		"clean-css": "^3.4.9",
		"core-js": "^2.3.0",
		"cross-env": "^1.0.7",
		"css-loader": "^0.23.1",
		"eventsource-polyfill": "^0.9.6",
		"express": "^4.13.4",
		"jasmine": "^2.4.1",
		"jasmine-core": "^2.4.1",
		"json-loader": "^0.5.4",
		"karma": "^0.13.21",
		"karma-chrome-launcher": "^0.2.2",
		"karma-html-reporter": "^0.2.7",
		"karma-jasmine": "^0.3.7",
		"karma-phantomjs-launcher": "^1.0.0",
		"karma-webpack": "^1.7.0",
		"node-sass": "^3.6.0",
		"rimraf": "^2.5.2",
		"sass-loader": "^3.1.2",
		"style-loader": "^0.13.1",
		"webpack": "^1.12.15",
		"webpack-dev-middleware": "^1.5.1",
		"webpack-hot-middleware": "^2.7.1"
	}
}
